<?xml version="1.0" encoding="utf-8"?>
<configuration>

	<!-- Custom 404 errors are forwarded to  -->
  <system.webServer>
    <httpErrors errorMode="Custom">
      <remove statusCode="404" />
      <error statusCode="404" path="/error/http404" responseMode="ExecuteURL" />
    </httpErrors>
  </system.webServer>

</configuration>